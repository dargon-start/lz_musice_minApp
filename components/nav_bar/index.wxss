/* baseui/nav-bar/index.wxss */
.nav-bar {
  display: flex;
  text-align: center;
  margin-top: 24rpx;
}

.left, .right {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 120rpx;
}

.left-arrow {
  display: none;
}

.left-slot:empty + .left-arrow {
  display: block;
}

.center-title {
  display: none;
}

.center-slot:empty + .center-title {
  display: block;
}

.left .left-arrow .icon {
  width: 44rpx;
  display: block;
}

.center {
  display: flex;
  align-items: center;
  justify-content: center;
  flex: 1;
}
.tab{
  color: red;
}

